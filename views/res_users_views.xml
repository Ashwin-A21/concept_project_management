<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_users_form_inherit_csl_roles" model="ir.ui.view">
        <field name="name">res.users.form.inherit.csl.roles</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            
            <xpath expr="//page[@name='access_rights']/group" position="before">
                <group string="Project (CSL) Role">
                    <field name="csl_project_role" widget="selection"/>
                </group>
            </xpath>


        </field>
    </record>
</odoo>